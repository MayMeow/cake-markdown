{
    "name": "maymeow/cakephp-markdown",
    "description": "Markdown plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0",
        "erusev/parsedown": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1.0"
    },
    "autoload": {
        "psr-4": {
            "Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Markdown\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
